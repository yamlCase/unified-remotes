<?xml version="1.0" encoding="utf-8"?>
<!-- <layout onLaunch="launch" onPause="pause"> -->
<layout color="956816">
  <grid>
    <row weight="2">
      <grid>
        <row >
          <button icon="power" onTap="close" />
          <button icon="fullscreen" onTap="fullscreen" />
        </row>
        <row >
          <button text="CC\nprev" onTap="sub_prev" />
          <button text="CC" onTap="sub_toggle" />
          <button text="CC\nnext" onTap="sub_next" />
        </row>
        <row >
          <button text="slower" onTap="sub_slower" />
          <button text="faster" onTap="sub_faster" />
        </row>
      </grid>
    </row>
    <row weight="3"> 
      <grid color="785801"> 
        <row>
          <button icon="back" onTap="back" /> 
          <button icon="up" onTap="up" /> 
          <button icon="playpause" onTap="play_pause" />
        </row>
        <row>
          <button icon="left" onTap="left" />
          <button icon="select" onTap="select" />
          <button icon="right" onTap="right" />
        </row>
        <row>
          <button icon="unlock" onTap="played_toggle" /> 
          <button icon="down" onTap="down" />
          <button icon="stop" onTap="stop" />
        </row>
      </grid>
    </row>
  </grid>
</layout>
